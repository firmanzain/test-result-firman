                        "maxLength": 6,
                        "pattern": "^\\d{6}$"
                    },
                    "name": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "email": {
                        "type": "string",
                        "format": "email"
                    },
                    "password": {
                        "type": "string",
                        "format": "password",
                        "minLength": 8,
                        "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[^A-Za-z0-9]).{8,}$"
                    }
                },
                "required": ["employee_number", "name", "password"]
            },
            "userCreateResponse": {
                "type": "object",
