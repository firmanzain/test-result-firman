        "parameters": {
            "pageParams": {
                "name": "pageParams",
                "in": "query",
                "description": "Page parameters",
                "schema": {
                    "type": "number",
                    "example": 1
                }
            },
            "limitParams": {
                "name": "limitParams",
                "in": "query",
                "description": "Limit parameters",
                "schema": {
                    "type": "number",
                    "example": 10
                }
            },
            "searchParams": {
                "name": "searchParams",
                "in": "query",
                "description": "Search parameters",
                "schema": {
                    "type": "string",
                    "example": "Data"
                }
            },
            "idParams": {
                "name": "idParams",
                "in": "path",
