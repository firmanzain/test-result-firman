                            },
                            "employee_number": {
                                "type": "string"
                            },
                            "name": {
                                "type": "string"
                            },
                            "email": {
                                "type": "string"
                            },
                            "created_at": {
                                "type": "string",
                                "format": "date-time"
                            }
                        },
                        "required": ["id", "employee_number", "name"]
                    }
                },
                "required": ["success", "message", "data"]
            },
            "userUpdateRequest": {
                "type": "object",
