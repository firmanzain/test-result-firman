                    "success": {
                        "type": "boolean"
                    },
                    "message": {
                        "type": "string"
                    },
                    "errors": {
                        "type": "object"
                    }
                },
                "required": ["success", "message", "errors"]
            },
            "loginRequest": {
                "type": "object",
                "properties": {
                    "employee_number": {
                        "type": "string",
                        "minLength": 6,
                        "maxLength": 6
                    },
                    "password": {
                        "type": "string"
                    }
                },
                "required": ["employee_number", "password"]
            },
            "loginResponse": {
                "type": "object",
                "properties": {
                    "success": {
                        "type": "boolean"
                    },
                    "message": {
                        "type": "string"
                    },
                    "data": {
                        "type": "object",
                        "properties": {
                            "token": {
                                "type": "string"
                            },
                            "user": {
                                "type": "object",
                                "properties": {
                                    "id": {
                                        "type": "integer"
                                    },
                                    "employee_number": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    }
                                },
                                "required": ["id", "employee_number", "name"]
                            }
                        },
                        "required": ["token", "user"]
                    }
                },
                "required": ["success", "message"]
            },
            "logoutRequest": {
                "type": "object"
